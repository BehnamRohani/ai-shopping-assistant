    "import requests\n",
    "\n",
    "url = \"http://127.0.0.1:8000/chat\"\n",
    "data = {\n",
    "    \"chat_id\": \"1\",\n",
    "    \"messages\": [{\"type\": \"text\", \"content\": \"return member random key: 5\"}]\n",
    "}\n",
    "\n",
    "# Disable system proxy\n",
    "resp = requests.post(url, json=data, proxies={\"http\": None, \"https\": None})\n",
    "print(resp.json())"
   ]
